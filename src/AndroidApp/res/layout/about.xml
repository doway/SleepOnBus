<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="fill_parent">
	<ImageView android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:id="@+id/imgAbout"
		android:layout_gravity="center_horizontal" android:src="@drawable/joanna"></ImageView>
	<TextView android:text="@string/team" android:id="@+id/tvTeam"
		android:layout_width="wrap_content" android:layout_height="wrap_content"
		style="@style/middleContent"></TextView>
	<TextView android:text="" android:id="@+id/tvVersion"
		android:layout_width="wrap_content" android:layout_height="wrap_content"
		style="@style/middleContent"></TextView>
	<ImageView android:layout_width="fill_parent" android:src="@drawable/dowill_logo"
		android:layout_height="wrap_content" android:id="@+id/imgDowillLog"
		android:background="#FFFFFF" android:onClick="imgDowillLogoOnClick"></ImageView>
</LinearLayout>
